<?xml version="1.0"?>
<robot name="cylinder">
  <link name="base_link">
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.1" ixy="0.0" ixz="0.0"
               iyy="0.1" iyz="0.0"
               izz="0.1"/>
      <origin xyz="0 0 0"/> <!-- Assuming the origin is at the center of the cylinder -->
    </inertial>
    <visual>
      <geometry>
        <cylinder length="0.6" radius="0.2"/>
      </geometry>
    </visual>
  </link>
</robot>
